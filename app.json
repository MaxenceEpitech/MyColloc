{
    "expo": {
        "name": "MyColloc",
        "slug": "MyColloc",
        "privacy": "public",
        "sdkVersion": "34.0.0",
        "platforms": ["ios", "android", "web"],
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true
        }
    },
    "ios": {
        "bundleIdentifier": "com.myColloc",
        "infoPlist": {
            "NSFaceIDUsageDescription": "This app will optionally use Face ID or Touch ID to save login",
            "NFCReaderUsageDescription": "Check card"
        }
    },
    "android": {
        "package": "com.myColloc"
    }
}
